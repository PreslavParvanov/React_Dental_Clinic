@model IEnumerable<DentalServiceViewModel>
@{
    ViewData["Title"] = "Dental Services";
}
<div class="dental-services-logo">
    <img class="home-img" src="https://res.cloudinary.com/ddfrnob2x/image/upload/v1671286978/Services_jwslxc.jpg" />
</div>

<h1 class="dental-service-h1">Dental Service</h1>
</br>

    @{
        foreach (var dentalService in Model)
        {
        <div class="container h-100 d-flex justify-content-center">
            <details class="info-details">
                <summary class="summary-info">@dentalService.ServiceName</summary>
                @dentalService.ServiceDescription
            </details>
        </div>
        }
    }

